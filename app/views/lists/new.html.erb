<div class="container col-6 my-5">

<h1>Create a list</h1>

<%= simple_form_for(@list) do |f| %>
  <%= f.input :name %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Create list", class: "btn btn-primary" %>
<% end %>

<%= link_to "See all lists", lists_path, class: "btn btn-primary" %>

</div>

